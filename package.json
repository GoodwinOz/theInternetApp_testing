{
  "name": "internetapp.testing",
  "version": "1.0.0",
  "description": "testing https://the-internet.herokuapp.com/ with seleniumJS & chromedriver",
  "main": "index.test.js",
  "scripts": {
    "test": "jest internet.test.js -- -t '#auth'",
    "test:auth": "npm run test -- -t '#auth'", 
    "test:download": "npm run test -- -t '#download'",
    "test:wdio": "wdio",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand internet.test.js",
    "test:watch": "jest --watch",
    "test:stop": "jest --forceExit"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "selenium",
    "theInternetApp",
    "chromedriver"
  ],
  "author": "Goodwin",
  "license": "MIT",
  "dependencies": {
    "@wdio/cli": "^7.2.3",
    "chai": "^4.3.4",
    "jasmine": "^3.7.0",
    "chromedriver": "^89.0.0",
    "dotenv": "^8.2.0",
    "jest-environment-selenium": "^2.1.2",
    "mocha": "^8.3.2",
    "nodemon": "^2.0.7",
    "selenium": "^2.20.0",
    "selenium-webdriver": "^4.0.0-beta.2",
    "webdriver-manager": "^12.1.8",
    "webdriverio": "^7.2.3"
  },
  "devDependencies": {
    "@wdio/jasmine-framework": "^7.2.3",
    "@wdio/local-runner": "^7.2.3",
    "@wdio/selenium-standalone-service": "^7.2.1",
    "@wdio/spec-reporter": "^7.2.1",
    "jest": "^26.6.3",
    "wdio-image-comparison-service": "^2.2.0",
    "wdio-novus-visual-regression-service": "^1.2.4"
  }
}
