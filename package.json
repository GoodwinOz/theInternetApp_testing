{
  "name": "internetapp.testing",
  "version": "1.0.0",
  "description": "testing https://the-internet.herokuapp.com/ with seleniumJS & chromedriver",
  "main": "index.test.js",
  "scripts": {
    "start": "node index.test.js",
    "test": "jest",
    "test:debug": "node --inspect node_modules/.bin/jest --watch --runInBand index.test.js",
    "test:watch": "jest --watch",
    "test:stop": "jest --forceExit"
  },
  "jest": {
    "verbose": true,
    "testPathIgnorePatterns": [
      "/node_modules/"
    ]
  },
  "keywords": [
    "selenium",
    "theInternetApp",
    "chromedriver"
  ],
  "author": "Goodwin",
  "license": "MIT",
  "dependencies": {
    "chromedriver": "^89.0.0",
    "dotenv": "^8.2.0",
    "jest-environment-selenium": "^2.1.2",
    "nodemon": "^2.0.7",
    "selenium": "^2.20.0",
    "selenium-webdriver": "^4.0.0-beta.2",
    "webdriver-manager": "^12.1.8"
  },
  "devDependencies": {
    "jest": "^26.6.3"
  }
}
